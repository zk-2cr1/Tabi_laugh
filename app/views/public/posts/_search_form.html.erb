<!--検索フォーム-->
<%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.text_field :keyword, :placeholder =>"投稿検索"  %>
    <%= f.select :method , options_for_select({"部分一致" => "partial", "前方一致" => "forward", "後方一致" => "backward", "完全一致" => "perfect" }) %>
    <%= f.submit '検索', class: "btn btn-dark btn-sm"%>
<% end %>